#include <iostream>
#include <cstring>
#include <algorithm>

typedef unsigned long long ULL;

// 字符串长度
const int N = 100010;
using namespace std;

char str[N];
ULL p[N], h[N];
int n, m, P = 131;

ULL get(int l, int r) {
    return h[r] - h[l - 1] * p[r - l + 1];
}

void do(string str) {
  p[0] = 1;
  for (int i = 1; i <= n; i++) {
      p[i] = p[i - 1] * P;
      h[i] = h[i - 1] * P + str[i];
  }
}

int main()
{
    scanf("%d%d%s", &n, &m, str + 1);
    do(str);
    while (m -- ) {
        int l1, l2, r1, r2;
        scanf("%d%d%d%d", &l1, &r1, &l2, &r2);
        if (get(l1, r1) == get(l2, r2)) puts("Yes");
        else puts("No");
    }
    return 0;
}
